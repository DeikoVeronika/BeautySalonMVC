@model IEnumerable<BeautySalonDomain.Model.Employee>

<h2>All Employees</h2>

<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Position</th>
            <!-- Додайте інші поля, які ви хочете відобразити -->
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td>@employee.Name</td>
                <td>@employee.Positions.Name</td>
                <!-- Додайте інші поля, які ви хочете відобразити -->
            </tr>
        }
    </tbody>
</table>
